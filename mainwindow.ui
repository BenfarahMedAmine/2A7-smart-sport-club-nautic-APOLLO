     <property name="styleSheet">
      <string notr="true">/* Style du QFrame */
QFrame {
  background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(85, 98, 112, 255), stop:1 rgba(255, 107, 107, 255));
border-radius:10px
}

/* Style des boutons */
QPushButton {
    background: qradialgradient(cx: 0.3, cy: -0.4, fx: 0.3, fy: -0.4, radius: 1.35, stop: 0 rgba(255, 255, 255, 255), stop: 1 rgba(200, 200, 200, 255)); /* Dégradé radial */
    border-radius: 20px; /* Coin arrondi */
    color: rgb(70, 70, 70); /* Couleur du texte */
    font: bold 12pt &quot;Arial&quot;; /* Police en gras, taille 12pt, Arial */
    padding: 10px 20px; /* Espacement interne */
    border: 2px solid rgba(150, 150, 150, 0); /* Bordure transparente */
    transition: all 0.3s ease; /* Transition fluide */
}

/* Style des boutons au survol */
QPushButton:hover {
    background: qradialgradient(cx: 0.3, cy: -0.4, fx: 0.3, fy: -0.4, radius: 1.35, stop: 0 rgba(255, 255, 255, 255), stop: 1 rgba(220, 220, 220, 255)); /* Dégradé radial légèrement plus foncé */
    border-color: rgba(100, 100, 100, 0.5); /* Bordure légèrement visible au survol */
    color: rgb(50, 50, 50); /* Couleur du texte légèrement plus foncée */
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.3); /* Légère ombre portée au survol */
}

/* Style des boutons quand pressés */
QPushButton:pressed {
    background: qradialgradient(cx: 0.3, cy: -0.4, fx: 0.3, fy: -0.4, radius: 1.35, stop: 0 rgba(255, 255, 255, 255), stop: 1 rgba(180, 180, 180, 255)); /* Dégradé radial légèrement plus foncé au clic */
    border-color: rgba(50, 50, 50, 0.5); /* Bordure légèrement plus foncée au clic */
    color: rgb(30, 30, 30); /* Couleur du texte encore plus foncée */
    box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.5); /* Ombre intérieure au clic */
}

/* Style des boutons désactivés */
QPushButton:disabled {
    background: qradialgradient(cx: 0.3, cy: -0.4, fx: 0.3, fy: -0.4, radius: 1.35, stop: 0 rgba(255, 255, 255, 255), stop: 1 rgba(240, 240, 240, 255)); /* Dégradé radial pour les boutons désactivés */
    border-color: rgba(200, 200, 200, 0.5); /* Bordure pour les boutons désactivés */
    color: rgba(150, 150, 150, 0.5); /* Couleur du texte pour les boutons désactivés */
    box-shadow: none; /* Pas d'ombre pour les boutons désactivés */
}
</string>
